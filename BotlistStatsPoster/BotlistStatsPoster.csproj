<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Daniel Baynton (230Daniel)</Authors>
    <Company />
    <Description>A .NET library to post a Discord bot's server count to various bot listing websites. See the complete list on the GitHub page.</Description>
    <Copyright>Daniel Baynton 2020</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIcon>DiscordLogo.png</PackageIcon>
    <RepositoryUrl>https://github.com/230Daniel/BotlistStatsPoster</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>MIT.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/230Daniel/BotlistStatsPoster</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Documents\Licenses\MIT.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\..\..\Pictures\DiscordLogo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
